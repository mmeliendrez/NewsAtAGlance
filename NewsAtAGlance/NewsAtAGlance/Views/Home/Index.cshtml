@{
    ViewBag.Title = "News At A Glance";
}

<h2>@ViewBag.Message</h2>

<div id="container">       
    <div id="header">
       <div id="aaglogo">
            <img src="/Content/Images/aag_logo.png" />
        </div>
        <div id="aagTopRight">
            <div id="buttons">
                <img id="gridButton" disabled class="unselected" src="/Content/Images/grid.png" />
                <img id="cloudButton" class="enabled"  src="/Content/Images/cloud.png"  />
            </div>
        </div>
    </div>        
    <div id="left" class="scroller"></div>
    <div id="right" class="scroller"></div>
    <div id="content"></div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        startup.init('Tandil');
    });

</script>